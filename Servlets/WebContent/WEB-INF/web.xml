<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlets</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet> 
	<servlet-name>govardhan</servlet-name>
	<servlet-class>com.Servlets.servlet.LoginServlet</servlet-class> 
  </servlet>
   <servlet> 
	<servlet-name>gova</servlet-name>
	<servlet-class>com.Servlets.servlet.UserDataServlet</servlet-class> 
  </servlet>
  <servlet-mapping>
	<servlet-name>govardhan</servlet-name>
	<url-pattern>/login.xyz</url-pattern>  
  </servlet-mapping>
    <servlet-mapping>
	<servlet-name>gova</servlet-name>
	<url-pattern>/userData.xyz</url-pattern>  
  </servlet-mapping>
</web-app>